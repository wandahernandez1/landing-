{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd landing-collection && npm install && npm run build",
  "outputDirectory": "landing-collection/dist",
  "framework": null,
  "installCommand": "echo 'Skip root install'",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
